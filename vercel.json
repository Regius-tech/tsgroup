{
  "version": 2,
  "routes": [
    {
      "src": "/api/positions",
      "dest": "api/positions.js"
    },
    {
      "src": "/api/chat",
      "dest": "api/chat.js"
    },
    {
      "src": "/logo(.*)",
      "dest": "/public/logo$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "builds": [
    { "src": "api/positions.js", "use": "@vercel/node" },
    { "src": "api/chat.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "public/**", "use": "@vercel/static" }
  ]
}














